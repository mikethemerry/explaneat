{
    "experiment": {
        "name": "Backache classification benchmark",
        "description": "Backache classification using NEAT with backpropagation",
        "codename": "backache_neat",
        "base_location": "./data/experiments/backache",
        "base_location_gpu": "./data/experiments/backache"
    },
    "results": {
        "location": "data",
        "database": "./data/experiments/backache/data/resultsDatabase.csv",
        "database_gpu": "./data/experiments/backache/data/resultsDatabase.csv"
    },
    "data": {
        "dataset_names": ["backache"],
        "locations": {
            "backache": "./data/pmlb/backache/"
        },
        "raw_location": "./data/pmlb/backache/backache.data",
        "raw_data_meta": "./data/pmlb/backache/backache-meta.json",
        "raw_location_gpu": "./data/pmlb/backache/backache.data",
        "raw_data_meta_gpu": "./data/pmlb/backache/backache-meta.json",
        "processed_location": "./data/processed/"
    },
    "model": {
        "config_file": "backache_neat_config",
        "svm": {
            "hyperparameter_ranges": {
                "C": [
                    0.1,
                    1,
                    10,
                    100,
                    1000
                ],
                "gamma": [
                    1,
                    0.1,
                    0.01,
                    0.001,
                    0.0001
                ],
                "kernel": [
                    "rbf"
                ]
            }
        },
        "random_forest": {
            "n_estimators": 100,
            "hyperparameter_ranges": {
                "n_estimators": [
                    10,
                    50,
                    100,
                    200,
                    500,
                    1000
                ],
                "max_depth": [
                    2,
                    5,
                    10,
                    15
                ],
                "min_samples_split": [
                    2,
                    5,
                    10,
                    15,
                    20
                ]
            }
        },
        "regression": {},
        "neural_network": {
            "batch_size": 32,
            "learning_rate": 0.001,
            "num_epochs": 10000,
            "n_iterations": 10
        },
        "propneat": {
            "n_iterations": 3,
            "population_size": 50,
            "epochs_per_generation": 10,
            "max_n_generations": 50,
            "base_config_path": "./configs/config-backache"
        },
        "propneat_retrain": {
            "n_epochs": 1000,
            "n_iterations": 5
        }
    },
    "hyperparam_tuning": {
        "n_iter": 100,
        "cv": 5,
        "scoring": "roc_auc"
    },
    "random_seed": 42,
    "train_test_ratio": 0.3
}